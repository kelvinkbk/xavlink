{
  "name": "xavlink-monorepo",
  "version": "1.0.0",
  "description": "XavLink - Campus Skills Marketplace",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm -C backend start\" \"npm -C web run dev\" \"npm -C mobile start\" --kill-others-on-fail",
    "dev:backend": "npm -C backend start",
    "dev:web": "npm -C web run dev",
    "dev:mobile": "npm -C mobile start",
    "build:web": "npm -C web run build",
    "build:mobile": "npm -C mobile run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "expo": "^54.0.33"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.8.12",
    "@react-navigation/native": "^7.1.25",
    "@react-navigation/native-stack": "^7.8.6",
    "axios": "^1.13.2",
    "expo-application": "^7.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-notifications": "^0.28.1",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "^6.0.9",
    "expo-updates": "^29.0.16",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.16.0",
    "socket.io-client": "^4.8.1"
  }
}
